@model C3Spectra.APPositioningApp.Entity.ManageDashboard.ManageDashboardViewModel
@{
    ViewData["Title"] = "Dashboard";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=device-width, initial-scale=1">
<link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.2/css/bootstrap.min.css" rel="stylesheet" />
<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script>
    $(document).ready(function () {
        $('#btnIndoor').click(function () {
            window.location.href = "/Indoor/IndoorStep?floorID=23";
        });

        $('#btnOutdoor').click(function () {
            window.location.href = "/Outdoor/OutdoorStep1";
        });
    });
</script>
<br />
<br />

<div class="row">
    <div class="col-md-6">
        <div class="form-group">
            <label class="col-xs-4">Select SAS</label>
            <div class="col-md-8">
                @Html.DropDownListFor(m => m.Sas.SASID, Model.Sas.SASNames, new { @class = " form-control", id = "ddSAS" })

            </div>
        </div>
    </div>


    <div class="col-md-6">
        <div class="form-group">
            <label class="col-xs-4">Select CBSD Vendor / Model</label>
            <div class="col-md-8">
                @Html.DropDownListFor(m => m.CBSDVM.CBSDVendorModelID, Model.CBSDVM.CBSDVendorModels, new { @class = " form-control", id = "ddCBSDHW" })

            </div>
        </div>
    </div>


</div>



<br />
<h3>Dashboard</h3>
<br />



<div class="row">
    <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12 col-12">
        <div class="card border-top border-top-primary">
            <div class="card-body">

                <button type="button" class="btn btn-warning" id="btnIndoor">Indoor </button>


            </div>



        </div>


    </div>
    <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12 col-12">


        <div class="card border-top border-top-primary">
            <div class="card-body">

                <button type="button" class="btn btn-primary" id="btnOutdoor">Outdoor </button>


            </div>



        </div>
    </div>

</div>
<script>
    $(document).ready(function () {
        $("#ddSAS").on("change", function () {
            $.ajax(
                {
                    url: '/Dashboard/Dashboard1?id=' + $('#ddSAS').val(),
                    type: 'GET',
                    data: "",
                    contentType: 'application/json; charset=utf-8',
                    success: function (data) {

                    },
                    error: function () {
                        alert("error");
                    }
                });
        });

        $("#ddCBSDHW").on("change", function () {
            $.ajax(
                {
                    url: '/Dasboard/Dashboard2?id=' + $('#ddCBSDHW').val(),
                    type: 'GET',
                    data: "",
                    contentType: 'application/json; charset=utf-8',
                    success: function (data) {

                    },
                    error: function () {
                        alert("error");
                    }
                });
        });
    });

</script>
